<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="Pranit.Listeners"></listener>
    </listeners>
    <test name="Login Test">
        <classes>
            <class name="Pranit.TestNG">
                <methods>
                    <include name="setup"/>
                    <include name="login"/>
                    <include name="teardown"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>